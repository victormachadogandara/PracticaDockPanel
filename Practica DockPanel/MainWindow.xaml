<Window x:Class="Practica_DockPanel.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Practica_DockPanel"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <DockPanel>
        <Button DockPanel.Dock="Left">Boton Uno</Button>
        <Button DockPanel.Dock="Left">Boton Cinco</Button>
        <Button DockPanel.Dock="Right">Boton Tres</Button>
        <Button DockPanel.Dock="Top">Boton Dos</Button>
        <Button DockPanel.Dock="Bottom">Boton Cuatro</Button>
        <StackPanel>
            <TextBlock Text="Esto es un texto"/>
            <TextBox/>
            <TextBlock Text="Esto es un texto"/>
            <Button DockPanel.Dock="Bottom">Boton Seis</Button>
        </StackPanel>
    </DockPanel>
</Window>
